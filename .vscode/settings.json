{
    "files.eol": "\n",
    "editor.tabSize": 4,
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "eslint.validate": [
        "javascript", "typescript"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "git.ignoreLimitWarning": true,
}